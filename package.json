{
  "name": "dinamo",
  "version": "0.0.1-alpha.12",
  "description": "Amazon DynamoDB utilities for Node.js",
  "author": "Rafael Franco",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "esbuild index.ts --outfile=dist/index.js --bundle --minify --platform=node",
    "prebuild": "rimraf dist",
    "postbuild": "npm run types",
    "types": "tsc --project tsconfig.build.json"
  },
  "keywords": [
    "aws-sdk",
    "dynamodb",
    "node",
    "node.js"
  ],
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.179.0",
    "@aws-sdk/lib-dynamodb": "^3.179.0",
    "camelcase": "^7.0.0"
  },
  "devDependencies": {
    "@types/node": "^18.7.23",
    "esbuild": "^0.15.9",
    "rimraf": "^3.0.2",
    "typescript": "^4.8.3"
  }
}
